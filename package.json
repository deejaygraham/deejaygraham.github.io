{
  "type": "module",
  "scripts": {
    "sass": "sass --style=compressed --load-path=node_modules src/assets/sass/site.scss _site/css/site.css",
    "social-images": "eleventy-social-images --siteName 'deejaygraham.github.io' --theme 'minimal'  --outputDir '_site/img' --templatePath 'social/template.html' --stylesPath 'social/style.css' --dataFile 'social/pages.json' ",
    "build": "npm run sass && eleventy",
    "start": "npm-run-all sass --parallel watch:*",
    "test": "npx cypress run --e2e --browser chrome",
    "unit-test": "npx ava --verbose",
    "format": "prettier . --write",
    "lint": "npx eslint ./src/_11ty"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^4.0.2",
    "@11tyrocks/eleventy-plugin-social-images": "^0.2.0",
    "ava": "^6.1.2",
    "bulma": "^1.0.0",
    "cypress": "^13.7.2",
    "elasticlunr": "^0.9.5",
    "prettier": "^3.2.5",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@eslint/js": "^9.15.0",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "install": "^0.13.0",
    "luxon": "^3.4.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.75.0"
  },
  "ava": {
    "files": [
      "./src/_11ty/filters/**/*.test.js"
    ]
  }
}
